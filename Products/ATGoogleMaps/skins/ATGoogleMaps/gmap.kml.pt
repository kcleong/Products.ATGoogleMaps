<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://earth.google.com/kml/2.0"
     xmlns:tal="http://xml.zope.org/namespaces/tal">
<tal:dummy tal:define="dummy python:request.RESPONSE.setHeader('Content-Type', 'application/vnd.google-earth.kml;; charset=utf-8')" />
<Document>
  <name tal:content="context/pretty_title_or_id">Document title</name>
  <description tal:content="context/description">Description</description>
  <LookAt>
    <longitude tal:content="context/center/longitude">139.71</longitude>
    <latitude tal:content="context/center/latitude">35.73</latitude>
  </LookAt>
  <Placemark tal:repeat="context python:context.objectValues('GMarker')">
    <name tal:content="context/pretty_title_or_id">Marker title</name>
    <description tal:content="context/kml_description">Description</description>
    <Point>
      <coordinates tal:content="string:${context/point/longitude},${context/point/latitude}">139.71,35.73</coordinates>
    </Point>
  </Placemark>
</Document>
</kml>
